{
  "name": "masonitedoors/wp-plugin-boilerplate",
  "description": "A WordPress plugin boilerplate for Masonite.",
  "type": "wordpress-plugin",
  "license": "GPL-2.0-or-later",
  "minimum-stability": "dev",
  "require": {
    "php": ">=7",
    "composer/installers": "^1.0"
  },
  "require-dev": {
    "automattic/phpcs-neutron-standard": "dev-master",
    "squizlabs/php_codesniffer": "^3.0@dev",
    "dealerdirect/phpcodesniffer-composer-installer": "dev-master",
    "phpcompatibility/php-compatibility": "dev-master",
    "wp-coding-standards/wpcs": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Masonite\\WP_Plugin_Boilerplate\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Masonite\\Tests\\WP_Plugin_Boilerplate\\": "tests/"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpcs",
    "fix": "./vendor/bin/phpcbf"
  }
}
